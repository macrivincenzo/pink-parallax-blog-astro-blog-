  "characteristics": [
    "Asset-heavy operations",
    "Production capacity limits",
    "Supply chain dependencies",
    "Regulatory compliance",
    "Equipment depreciation"
  ],
  "key_factors": [
    "Production efficiency",
    "Asset condition",
    "Market demand",
    "Supply chain stability",
    "Regulatory compliance"
  ]
};
---

<CalculatorTemplate industry={industry} />
