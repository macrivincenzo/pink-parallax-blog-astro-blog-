---
import CalculatorTemplate from '../components/CalculatorTemplate.astro';

const industry = {
  "id": "manufacturing",
  "name": "Manufacturing Business",
  "title": "Manufacturing Business Calculator - Value Your Manufacturing Company Today",
  "description": "Comprehensive manufacturing business valuation calculator with asset-based analysis, production capacity evaluation, and industry-specific multiples.",
  "keywords": "manufacturing valuation calculator, manufacturing business value, industrial company valuation, manufacturing company worth, factory valuation",
  "multiple_range": {
    "min": 1,
    "max": 2.2
